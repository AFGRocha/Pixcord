{
    "name": "Pixcord Closed EN Build",
    "version": "1.1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "icons": { "16": "pixcord-icon.png",
        "48": "pixcord-icon.png",
       "128": "pixcord-icon.png" },
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Pixcord"
    },
    "options_page": "options.html",
    "permissions": [
        "storage"
      ]
}